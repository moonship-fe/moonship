    "@moonship-fe/slate-hyperscript": "*",
    "@moonship-fe/slate-react": "*",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
